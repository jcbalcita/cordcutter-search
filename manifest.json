{
  "manifest_version": 2,

  "name": "Cordcutter Search",
  "description": "For cordcutters who constantly ask - 'where can I stream it?'",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "cordcutter.png",
    "default_title": "Cordcutter Search"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "http://*.guidebox.com"
  ]
}
